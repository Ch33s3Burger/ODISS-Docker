{
  "type": "kafka",
  "spec": {
    "ioConfig": {
      "type": "kafka",
      "consumerProperties": {
        "bootstrap.servers": "kafka:9092",
        "security.protocol": "SASL_PLAINTEXT",
        "sasl.mechanism": "PLAIN",
        "sasl.jaas.config": "org.apache.kafka.common.security.plain.PlainLoginModule required username='${ODISS_KAFKA_KAFKASERVER_USERNAME}' password='${ODISS_KAFKA_KAFKASERVER_PASSWORD}';"
      },
      "topic": "subjectentry",
      "inputFormat": {
        "type": "json"
      },
      "useEarliestOffset": true,
      "taskDuration": "PT30M",
      "completionTimeout": "PT30M"
    },
    "tuningConfig": {
      "type": "kafka"
    },
    "dataSchema": {
      "dataSource": "subjectentry",
      "timestampSpec": {
        "column": "EntryDateTimeStamp",
        "format": "iso"
      },
      "dimensionsSpec": {
        "dimensions": [
          "NewestEntry",
          "AuditLogID",
          "EntryOldValue",
          "EntryValueText",
          "StudyEventStartDate",
          "StudyEventEndDate",
          "UserOID",
          "ItemOID",
          {
            "type": "long",
            "name": "ItemGroupRepeatKey"
          },
          "StudyEventOID",
          "ItemGroupOID",
          "FormOID",
          "EntryAuditType",
          "ItemGroupTransactionType",
          "SubjectAgeAtStudyEvent",
          "SubjectKey",
          "StudyOID",
          "EntryReasonForChange",
          "FormStatus",
          "EntryValue",
          "FormVersion",
          "StudyEventStatus",
          "AuditLogsEntityID"
        ]
      },
      "granularitySpec": {
        "queryGranularity": "none",
        "rollup": false,
        "segmentGranularity": "day"
      }
    }
  }
}
