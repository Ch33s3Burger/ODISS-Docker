{
  "type": "kafka",
  "spec": {
    "ioConfig": {
      "type": "kafka",
      "consumerProperties": {
        "bootstrap.servers": "kafka:9092",
        "security.protocol": "SASL_PLAINTEXT",
        "sasl.mechanism": "PLAIN",
        "sasl.jaas.config": "org.apache.kafka.common.security.plain.PlainLoginModule required username='${ODISS_KAFKA_KAFKASERVER_USERNAME}' password='${ODISS_KAFKA_KAFKASERVER_PASSWORD}';"
      },
      "topic": "subjectentry",
      "inputFormat": {
        "type": "json"
      },
      "useEarliestOffset": true
    },
    "tuningConfig": {
      "type": "kafka"
    },
    "dataSchema": {
      "dataSource": "subjectentry",
      "timestampSpec": {
        "column": "entryDateTimeStamp",
        "format": "iso"
      },
      "dimensionsSpec": {
        "dimensions": [
          "studyEventEndDate",
          "entryValueText",
          "entryAuditType",
          {
            "type": "double",
            "name": "formVersion"
          },
          {
            "type": "double",
            "name": "entryValue"
          },
          {
            "type": "long",
            "name": "itemGroupRepeatKey"
          },
          "entryReasonForChange",
          "subjectAgeAtStudyEvent",
          "userID",
          "studyEventOID",
          "newestEntry",
          "itemOID",
          "auditLogID",
          "formOID",
          "formStatus",
          "studyEventStatus",
          "studyEventStartDate",
          "auditLogsEntityID",
          "studyOID",
          "entryOldValue",
          "itemGroupOID",
          "itemGroupTransactionType",
          "subjectKey"
        ]
      },
      "granularitySpec": {
        "queryGranularity": "none",
        "rollup": false,
        "segmentGranularity": "day"
      }
    }
  }
}
