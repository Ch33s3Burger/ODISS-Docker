{
  "type": "kafka",
  "spec": {
    "ioConfig": {
      "type": "kafka",
      "consumerProperties": {
        "bootstrap.servers": "kafka:9092",
        "security.protocol": "SASL_PLAINTEXT",
        "sasl.mechanism": "PLAIN",
        "sasl.jaas.config": "org.apache.kafka.common.security.plain.PlainLoginModule required username='${ODISS_KAFKA_KAFKASERVER_USERNAME}' password='${ODISS_KAFKA_KAFKASERVER_PASSWORD}';"
      },
      "topic": "item",
      "inputFormat": {
        "type": "json"
      },
      "useEarliestOffset": true,
      "taskDuration": "PT10M",
      "completionTimeout": "PT10M"
    },
    "tuningConfig": {
      "type": "kafka"
    },
    "dataSchema": {
      "dataSource": "item",
      "timestampSpec": {
        "column": "!!!_no_such_column_!!!",
        "missingValue": "2010-01-01T00:00:00Z"
      },
      "dimensionsSpec": {
        "dimensions": [
          {
            "type": "long",
            "name": "SignificantDigits"
          },
          "Comment",
          "LeftItemText",
          "SASFieldName",
          {
            "type": "long",
            "name": "Length"
          },
          "MeasurementUnitText",
          "DataType",
          "MeasurementUnitName",
          "ItemOID",
          "Name",
          "MeasurementUnitOID"
        ]
      },
      "granularitySpec": {
        "queryGranularity": "none",
        "rollup": false,
        "segmentGranularity": "all"
      }
    }
  }
}
